    "object-path": "0.11.2",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.0",
    "mocha-lcov-reporter": "^1.2.0"
  }
